{
  "name": "frontend",
  "version": "0.0.36",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "node ./prebuild.js prod && ng build --configuration=production --base-href=/WHS/ && node ./postbuild.js",
    "build-qa": "node ./prebuild.js qa && ng build --configuration=qa --base-href=/WHS/ && node ./postbuild.js",
    "build-local": "node ./prebuild.js qa && ng build && node ./postbuild.js",
    "minor-version": "npm --no-git-tag-version version patch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api": "json-server --watch ./local/db.json --routes ./local/routes.json",
    "gh-deploy-build": "ng build --prod=true --outputPath=docs --baseHref=/deploy/ --deploy-url=/deploy/",
    "gh-deploy-copy": "cp docs/index.html docs/404.html",
    "gh-deploy": "start npm run gh-deploy-build && start npm run gh-deploy-copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.8",
    "@angular/material-moment-adapter": "^13.0.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "~12.2.0",
    "@dpogue/dexie": "^1.4.1",
    "@esri/arcgis-rest-auth": "^3.4.3",
    "@esri/arcgis-rest-geocoding": "^3.4.3",
    "@esri/arcgis-rest-request": "^3.4.3",
    "@esri/arcgis-rest-types": "^3.4.3",
    "@ngrx/store": "^12.4.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "@types/geojson": "^7946.0.8",
    "@types/uuid": "^8.3.2",
    "maplibre-gl": ">= 1.15.1",
    "moment": "^2.29.1",
    "ng-recaptcha": "^9.0.0",
    "ngx-dropzone": "^3.0.0",
    "ngx-maplibre-gl": "^12.0.0",
    "rfs": "^9.0.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "http-server": "^13.0.2",
    "jasmine-core": "~3.8.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
